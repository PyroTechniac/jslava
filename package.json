{
	"name": "jslava",
	"version": "0.0.1",
	"author": "Gryffon Bellish <owenbellish@gmail.com>",
	"description": "A lavaplayer and lavalink based library, written entirely for node",
	"license": "MIT",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"lint": "npx eslint src --ext .ts",
		"lint:fix": "npx eslint src --ext .ts --fix",
		"build": "tsc",
		"prebuild": "npm run lint:fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PyroTechniac/jslava.git"
	},
	"bugs": {
		"url": "https://github.com/PyroTechniac/jslava/issues"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^11.13.8",
		"@typescript-eslint/eslint-plugin": "^1.7.0",
		"@typescript-eslint/parser": "^1.7.0",
		"eslint": "^5.16.0",
		"eslint-config-marine": "^2.2.0",
		"typescript": "^3.4.5"
	},
	"optionalDependencies": {},
	"peerDependencies": {},
	"eslintConfig": {
		"extends": "marine/node",
		"rules": {
			"@typescript-eslint/indent": [
				"error",
				4,
				"spaces"
			],
			"indent": "off",
			"quotes": [
				"error",
				"single"
			],
			"multiline-ternary": "off",
			"radix": "off",
			"@typescript-eslint/promise-function-async": "off",
			"promise/prefer-await-to-callbacks": "off",
			"@typescript-eslint/no-extraneous-class": "off",
			"@typescript-eslint/no-object-literal-type-assertion": "off",
			"@typescript-eslint/no-parameter-properties": "off",
			"no-useless-constructor": "off",
			"@typescript-eslint/no-use-before-define": "off",
			"no-use-before-define": "off",
			"guard-for-in": "off"
		}
	},
	"eslintIgnore": [
		"lib/**"
	]
}